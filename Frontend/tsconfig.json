{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "Frontend/.expo/types/**/*.ts",
    "Frontend/expo-env.d.ts",
    "app/index.jsx",
    "app/(tabs)/home.jsx",
    "app/(tabs)/messages.jsx",
    "Frontend/nativewind-env.d.ts",
    "nativewind-env.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
